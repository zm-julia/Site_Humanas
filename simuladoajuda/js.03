let arr = []

function gerarAleatorio(min,max){
    return Math.floor(Math.random() * (max - min) + min)
}

function principal(){
let max = 20, min = 1
let soma = 0


for(let i = 0; i < 10; i++){
    arr[i] = gerarAleatorio(min,max)
    soma += arr[i]
}
console.table(arr)
console.log(soma)
}

principal()